{
  "files.associations": {
    "**/templates/**/*.html": "django-html",
    "**/templates/**/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "**/*.html": "html"
  },
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "database": "${workspaceFolder:blog}/db.sqlite3",
      "name": "simple_blog"
    }
  ]
}
